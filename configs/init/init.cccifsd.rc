#

on early-init

on post-fs-data

#SeLinux
    restorecon_recursive /vendor/protect_f
    restorecon_recursive /vendor/protect_s

#
#  Modem related services
#

service ccci_fsd /vendor/bin/ccci_fsd 0
    user radio
    group radio system
    class core
    oneshot

service ccci3_fsd /vendor/bin/ccci_fsd 2
    user radio
    group  radio system
    class core
    oneshot

